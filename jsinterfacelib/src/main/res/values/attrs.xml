<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SignatureView">
        <attr format="color" name="penColor"/>
        <attr format="color" name="backColor"/>
        <attr format="integer" name="penWidth"/>
    </declare-styleable>

    <!--自定义属性 -->
    <declare-styleable name="ClipViewLayoutJS">
        <attr name="clipTypeJS" format="enum">
            <enum name="circleJS" value="1" />
            <enum name="rectangleJS" value="2" />
        </attr>
        <attr name="mHorizontalPaddingJS" format="dimension" />
        <attr name="clipBorderWidthJS" format="dimension" />
    </declare-styleable>
</resources>